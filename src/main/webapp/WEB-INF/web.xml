<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <filter>
  	<filter-name>AnimatchFilter</filter-name>
  	<display-name>AnimatchFilter</display-name>
  	<description></description>
  	<filter-class>com.web01.animatch.filter.DefaultFilter</filter-class>
  </filter>
  <filter-mapping>
  	<filter-name>AnimatchFilter</filter-name>
  	<url-pattern>/*</url-pattern>
  </filter-mapping>
  <servlet>
    <servlet-name>IndexController</servlet-name>
    <display-name>IndexController</display-name>
    <description></description>
  	<servlet-class>com.web01.animatch.controller.IndexController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>LoginController</servlet-name>
  	<display-name>LoginController</display-name>
  	<description></description>
  	<servlet-class>com.web01.animatch.controller.LoginController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>SignupController</servlet-name>
  	<display-name>SignupController</display-name>
  	<description></description>
  	<servlet-class>com.web01.animatch.controller.SignupController</servlet-class>
  </servlet>
  <!--
  <servlet>
  	<servlet-name>MemberSearchOwnerController</servlet-name>
  	<display-name>MemberSearchOwnerController</display-name>
  	<description></description>
  	<servlet-class>com.web01.animatch.controller.MemberSearchOwnerController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>MemberSearchTrimmerController</servlet-name>
  	<display-name>MemberSearchTrimmerController</display-name>
  	<description></description>
  	<servlet-class>com.web01.animatch.controller.MemberSearchTrimmerController</servlet-class>
  </servlet>
  -->
  <servlet>
  	<servlet-name>MemberSearchController</servlet-name>
  	<display-name>MemberSearchController</display-name>
  	<description></description>
  	<servlet-class>com.web01.animatch.controller.MemberSearchController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>MemberDetailOwnerController</servlet-name>
  	<display-name>MemberDetailOwnerController</display-name>
  	<description></description>
  	<servlet-class>com.web01.animatch.controller.MemberDetailOwnerController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>MemberDetailTrimmerController</servlet-name>
  	<display-name>MemberDetailTrimmerController</display-name>
  	<description></description>
  	<servlet-class>com.web01.animatch.controller.MemberDetailTrimmerController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>MemberDmessageListController</servlet-name>
  	<display-name>MemberDmessageListController</display-name>
  	<description></description>
  	<servlet-class>com.web01.animatch.controller.MemberDmessageListController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>MemberDmessageDetailController</servlet-name>
  	<display-name>MemberDmessageDetailController</display-name>
  	<description></description>
  	<servlet-class>com.web01.animatch.controller.MemberDmessageDetailController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ContactController</servlet-name>
  	<display-name>ContactController</display-name>
  	<description></description>
  	<servlet-class>com.web01.animatch.controller.ContactController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>IndexController</servlet-name>
  	<url-pattern>/index</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>LoginController</servlet-name>
  	<url-pattern>/login/</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>SignupController</servlet-name>
  	<url-pattern>/signup/</url-pattern>
  </servlet-mapping>
  <!--
  <servlet-mapping>
  	<servlet-name>MemberSearchOwnerController</servlet-name>
  	<url-pattern>/member/search/owner</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>MemberSearchTrimmerController</servlet-name>
  	<url-pattern>/member/search/trimmer</url-pattern>
  </servlet-mapping>
  -->
  <servlet-mapping>
  	<servlet-name>MemberSearchController</servlet-name>
  	<url-pattern>/member/search/*</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>MemberDetailOwnerController</servlet-name>
  	<url-pattern>/member/detail/owner</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>MemberDetailTrimmerController</servlet-name>
  	<url-pattern>/member/detail/trimmer</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>MemberDmessageListController</servlet-name>
  	<url-pattern>/member/dmessage/list</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>MemberDmessageDetailController</servlet-name>
  	<url-pattern>/member/dmessage/detail/</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ContactController</servlet-name>
  	<url-pattern>/contact/</url-pattern>
  </servlet-mapping>

  <welcome-file-list>
    <welcome-file>index</welcome-file>
  </welcome-file-list>

  <error-page>
    <error-code>404</error-code>
    <location>/index</location>
  </error-page>
</web-app>

